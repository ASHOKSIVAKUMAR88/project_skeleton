.PHONY=all
.DEFAULT_GOAL=all
all: C C++
	
C: 
	@echo "-===============================-"
	@echo "-=====Generating C Objects======-"
	@echo "-===============================-"
	#$(MAKE) -C ./c/
C++:
	@echo "-===============================-"
	@echo "-=====Generating C++ Object=====-"
	@echo "-===============================-"
	$(MAKE) -C ./c++/


clean:
	@echo "-===============================-"
	@echo "-===========CLEANING============-"
	@echo "-===============================-"
	$(MAKE) $@ -C ./c/
	$(MAKE) $@ -C ./c++/
